			<class name="com.qualitesoft.freightclub.testscripts.manageorders.TestOpenManageOrdersAndSearchOrderId" />
			<class name="com.qualitesoft.freightclub.testscripts.manageorders.TestCloneOrders" />
		</classes>
	</test>
	
